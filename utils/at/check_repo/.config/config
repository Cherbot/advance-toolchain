# The place which stores all AT config files
set at_config_repo "http://pokgsa.ibm.com/home/t/u/tulioqm/web/public/at-config"

# Email settings
# Recipients of email alerts
set RECIPIENTS {
	tuliom@linux.vnet.ibm.com
}

# Specify how to send an email
# Valid values are:
# tcl - requires smtp from TCLlib
# mail - uses POSIX mail command (default)
set MAIL_SYS tcl
set SMTP_SERVER la.relay.ibm.com
